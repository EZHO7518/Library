package Book;

public class showBookList {
	private static void showBookList() {
	    // 간단한 도서 목록 배열을 가정합니다.
	    String[] bookList = {
	        "도서 1: 자바 프로그래밍 입문",
	        "도서 2: 데이터베이스 설계와 구현",
	        "도서 3: 알고리즘과 자료구조"
	    };

	    // 도서 목록을 콘솔에 출력합니다.
	    System.out.println("도서 목록:");
	    for (String book : bookList) {
	        System.out.println(book);
	    }
	}

}
